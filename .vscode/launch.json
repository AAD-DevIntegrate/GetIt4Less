{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations":[
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "LaunchTask": {
                "browser": "edge",
                "browserLaunchLocation": "127.0.0.1:5500",
                "request": "pull",
                "type": "fetch",
            },
        },
        {
            "program": "${workspaceFolder}/manage.py",
            "connect": { 
                "pwa-extensionHost": "attach",
                "hostname": "@",
                "cname": "partners-autoafterdark.com",
                "www": "127.0.0.1",
                "pauseForSourceMap": true},
                "name": "DevTools",
                "request": "launch",
                "type": "vscode-edge-devtools.debug",
                "url": "https://autoafterdark.jetbrains.space",                
        }
    ]
}